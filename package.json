{
  "name": "ng2-github-starter",
  "version": "0.0.1",
  "description": "Starter Angular 2 Github query",
  "main": "main.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" \"sass --watch ./src/styles/styles.scss:./src/styles/styles.css\"",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "author": "JakeS",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "@salesforce-ux/design-system": "^1.0.4",
    "angular2-in-memory-web-api": "0.0.9",
    "core-js": "^2.4.0",
    "es6-shim": "^0.35.1",
    "ng-lightning": "^0.9.0",
    "reflect-metadata": "^0.1.2",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "^0.19.28",
    "tether": "^1.3.2",
    "tiny-ng-store": "^0.1.1",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  }
}
